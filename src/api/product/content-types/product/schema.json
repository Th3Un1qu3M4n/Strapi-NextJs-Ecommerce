{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Laptops",
        "Phones",
        "Gadgets",
        "Accessories",
        "Components",
        "Display",
        "None"
      ],
      "default": "None",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "availableQty": {
      "type": "integer",
      "required": true
    }
  }
}
